library(shiny)
library(tidyverse)
library(ggplot2)
library(plotly)
library(dplyr)
# Load Data
base <- read_delim("WhatsgoodlyData-6.csv") #unedited/raw data
## DATA CLEANING
sample_n(base, 5)
colnames(base)
# Subset for Table Tab
shopping_uni <- base %>%
filter(`Segment Type` == "University") %>%
group_by(`Segment Description`)
input <- "University of Pennsylvania"
shopping_uni %>%
filter(`Segment Description` == input) %>%
select(Percentage) %>%
max() %>%
paste("The most popular app at", input, "was", .)
runApp()
